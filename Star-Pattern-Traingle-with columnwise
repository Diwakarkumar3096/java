package fundamentals;
import java.util.Scanner;
public class helloWorld{

	
		public static void main(String args[]) {
			Scanner scan=new Scanner(System.in);
			System.out.println("Enter the value of row and Colums");
			int n=scan.nextInt();
			
			int i=1;
			while(i<=n) {
				int p=i;
				int j=1;
				while(j<=i) {
					System.out.print(p);
					p++;
					j++;
					
				}
				System.out.println();
				i++;
				
				
				
			}
			

           
	}
}


Enter the value of row and Colums
5
1
23
345
4567
56789
