package fundamentals;
import java.util.Scanner;
public class helloWorld{

	
		public static void main(String args[]) {
			Scanner scan=new Scanner(System.in);
			System.out.println("Enter the value of row and Colums");
			int n=scan.nextInt();
			
			int i=1;
			while(i<=n) {
				char p=(char)('A'+i-1);
				int j=1;
				while(j<=n) {
					System.out.print(p);
					p=(char)(p+1);
					j++;
					
				}
				System.out.println();
				i++;
				
				
				
			}
			

           
	}
}


Enter the value of row and Colums
4
ABCD
BCDE
CDEF
DEFG
