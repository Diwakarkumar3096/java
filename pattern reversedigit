package fundamentals;
import java.util.Scanner;
public class helloWorld{

	
		public static void main(String args[]) {
			Scanner scan=new Scanner(System.in);
			System.out.println("Enter the value of row and Colums");
			int n=scan.nextInt();
			
			int i=1;
			while(i<=n) {
				
				int j=1;
				while(j<=n-i+1){
					System.out.print(j);
					j++;	
				}
				 System.out.println();
				 i++;
				
				
				
			}
			

           
	}
}
Enter the value of row and Colums
5
12345
1234
123
12
1
